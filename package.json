{
  "name": "parcel-reason-react-app",
  "version": "2.0.0",
  "description": "Parcel Bundler Example with Reason React",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jihchi/parcel-reason-react-app.git"
  },
  "scripts": {
    "build": "npm run build:bsb && parcel build src/index.html --public-url /",
    "build:bsb": "bsb -make-world -clean-world",
    "start": "concurrently --kill-others --names \"ReasonML,Parcel\" --prefix-colors \"red.bgBlack.bold,yellow.bgBlack.bold\" \"npm run watch:bsb\" \"npm run watch:parcel\"",
    "watch:bsb": "npm run build:bsb -- -w",
    "watch:parcel": "parcel src/index.html"
  },
  "dependencies": {
    "reason-react": "^0.3.1"
  },
  "devDependencies": {
    "bs-platform": "^2.1.0",
    "concurrently": "^3.5.1",
    "parcel-bundler": "^1.5.0"
  }
}
